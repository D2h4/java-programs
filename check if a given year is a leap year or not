import java.util.*;
class Test2
{
	public static void main(String arg[])
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the year: ");
		int y=s.nextInt();
		if(y%100==0 && y%400==0 || y%100!=0 && y%4==0)
		{
			System.out.println("leap year");
		}
		else
		{
			System.out.println("not a leap year");
		}
		
}
}
